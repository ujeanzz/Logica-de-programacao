#include <stdio.h>
int main()
{
    int idade, soma;
    double media, quantidade;

    soma=0;
    quantidade=0;

    while(1){
    scanf("%d", &idade);
    if(idade<0)
      break;
    else{
    soma+=idade;
    quantidade++;
        }
    }
    media = soma/quantidade;
    printf("%.2lf\n", media);

    return 0;
}

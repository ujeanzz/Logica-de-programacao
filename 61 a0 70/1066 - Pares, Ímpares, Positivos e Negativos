#include <stdio.h>

int main(void) {
  int N, cont, vp, vn, par, impar;
  cont=5;
  vp=0;
  vn=0;
  par=0;
  impar=0;

  for(cont=1; cont<=5; cont++){
    scanf("%d", &N);
    if(N>0)
    vp++;
    else if(N<0)
    vn++;
    if(N%2==0)
    par++;
    else
    impar++;
  }
    printf("%d valor(es) par(es)\n", par);
    printf("%d valor(es) impar(es)\n", impar);
    printf("%d valor(es) positivo(s)\n", vp);
    printf("%d valor(es) negativo(s)\n", vn);
  
  return 0;
}

#include <stdio.h>

int main() {
    int X = 0;
    int fuel=0;
    int alc=0;
    int gas=0;
    int die=0;
    
    while (X != 4) {
         
      scanf("%d", &X);
         if (X == 1) {
            alc+=1;
            }
            
         else if (X == 2) {
                gas+= 1;
            }
            
         else if (X == 3) {
                die += 1;
            }
        }
        printf("MUITO OBRIGADO\n");
        printf("Alcool: %d\n", alc);
        printf("Gasolina: %d\n", gas);
        printf("Diesel: %d\n", die);

    return 0;
}

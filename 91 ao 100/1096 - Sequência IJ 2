#include <stdio.h>

int main(void) {
  int n1, n2;
  n1=1;
  n2=7;

  for(n1=1, n2=7; n2<=7; n1=n1, n2-=1){
    printf("I=%d J=%d\n", n1, n2);
      if(n2==5)
        break;
  }
  n1=3;
  n2=7;

  for(n1=3, n2=7; n2<=7; n1=n1, n2-=1){
    printf("I=%d J=%d\n", n1, n2);
      if(n2==5)
        break;
  }
  n1=5;
  n2=7;

  for(n1=5, n2=7; n2<=7; n1=n1, n2-=1){
    printf("I=%d J=%d\n", n1, n2);
      if(n2==5)
        break;
  }
  n1=7;
  n2=7;

  for(n1=7, n2=7; n2<=7; n1=n1, n2-=1){
    printf("I=%d J=%d\n", n1, n2);
      if(n2==5)
        break;
  }
  n1=9;
  n2=7;

  for(n1=9, n2=7; n2<=7; n1=n1, n2-=1){
    printf("I=%d J=%d\n", n1, n2);
      if(n2==5)
        break;    
  }
  
  return 0;
}

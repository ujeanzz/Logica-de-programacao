#include <stdio.h>

int main(void){
  int n1, n2;
  scanf("%d", &n1);
  n2=n1*1;
  printf("1 x %d = %d\n", n1, n2);
  n2=n1*2;
  printf("2 x %d = %d\n", n1, n2);
  n2=n1*3;
  printf("3 x %d = %d\n", n1, n2);
  n2=n1*4;
  printf("4 x %d = %d\n", n1, n2);
  n2=n1*5;
  printf("5 x %d = %d\n", n1, n2);
  n2=n1*6;
  printf("6 x %d = %d\n", n1, n2);
  n2=n1*7;
  printf("7 x %d = %d\n", n1, n2);
  n2=n1*8;
  printf("8 x %d = %d\n", n1, n2);
  n2=n1*9;
  printf("9 x %d = %d\n", n1, n2);
  n2=n1*10;
  printf("10 x %d = %d\n", n1, n2);
  
  return 0;
}

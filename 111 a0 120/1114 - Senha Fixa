#include <stdio.h>

int main()
{
    int codigo;
    codigo = 1;

    while(codigo != 2002)
    {
        scanf("%d", &codigo);
        if(codigo == 2002){
            printf("Acesso Permitido\n" );
            return 0;
        }else{
            printf("Senha Invalida\n" );
        }
    }
    return 0;
}
